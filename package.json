{
  "name": "minesweeper",
  "version": "1.0.0",
  "description": "Minesweeper game written in TypeScript",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "build:local": "webpack --env.local",
    "build:vis": "webpack --env.visualize",
    "build:local:vis": "webpack --env.local --env.visualize",

    "build:dev": "webpack --mode development",
    "build:dev:vis": "webpack --mode development --env.visualize",

    "start:dev": "webpack-dev-server",
    "start:dev:vis": "webpack-dev-server --env.visualize",

    "start": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmccormack/minesweeper.git"
  },
  "keywords": [
    "Minesweeper",
    "Game",
    "TypeScript",
    "TS"
  ],
  "author": "https://gitter.im/FreeCodeCamp/HelpJavaScript",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmccormack/minesweeper/issues"
  },
  "homepage": "https://github.com/cmccormack/minesweeper#readme",
  "devDependencies": {
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "image-webpack-loader": "^6.0.0",
    "lint-staged": "^9.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  }
}
